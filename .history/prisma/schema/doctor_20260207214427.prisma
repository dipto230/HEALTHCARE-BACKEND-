model Doctor {
  id            String     @id @default(uuid(7))
  name          String
  email         String     @unique
  profilePhoto  String?
  contactNumber String?
  address       String?
  isDeleted     Boolean    @default(false)
  deletedAt     DateTime?
  registrationNumber String @unique
  experience Int  @default(0)
  gender       GE
  createdAt     DateTime   @default(now())
  updatedAt     DateTime   @updatedAt


}